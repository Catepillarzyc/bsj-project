	   if(flag == false){
	       alert("输入不合法，只能输入正整数");
		   return false;
	   }
	});
	$(function(){
	    if(jQuery("#oter").is(':checked')){
		    $("input[name=reasons]").removeAttr("readonly")
		}
	})
	$(function(){
	     $("#oter").click(function(){
            if(jQuery(this).is(':checked')){
        	    $("input[name=reasons]").removeAttr("readonly");		    
        	}else{
			    $("input[name=reasons]").val("");
