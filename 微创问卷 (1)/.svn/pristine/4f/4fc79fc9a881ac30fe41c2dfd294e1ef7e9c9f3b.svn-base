</div>
<div class="question">
    <div>
        <div class="fill-short"><span class="num">Q6.</span>手术时间</div><div class="fill-short-input"><input maxlength="3" value="$!{clinicalApplication.operationTime}"  name="operationTime" onkeypress="keyPress()" type="text" style="width:2em" class="center"/></div><div class="unit">min</div>
        <div class="comma">，</div>
        <div class="fill-short">出血</div><div class="fill-short-input"><input maxlength="4" value="$!{clinicalApplication.bleedingMl}" name="bleedingMl" type="text" onkeypress="keyPress()" style="width:2em" class="center"/></div><div class="unit">ml</div>
    </div>
    <div>
        <div class="fill-short">肝门阻断次数</div><div class="fill-short-input"><input maxlength="4" value="$!{clinicalApplication.portazsNumber}" name="portazsNumber" onkeypress="keyPress()" type="text" style="width:2em" class="center"/></div><div class="unit">次</div>
        <div class="comma">，</div>
        <div class="fill-short">时间</div><div class="fill-short-input"><input maxlength="3" value="$!{clinicalApplication.portazsTime}" name="portazsTime" type="text" onkeypress="keyPress()" style="width:2em" class="center"/></div><div class="unit">min</div>
    </div>
</div>
<div class="question">
    <div class="fill-long"><span class="num">Q7.</span>并发症</div>
    <div>
        <div class="fill-short">二次手术</div><div class="fill-short-input"><input maxlength="4" value="$!{clinicalApplication.twoSurgeryNumber}" name="twoSurgeryNumber" onkeypress="keyPress()" type="text" style="width:2em" class="center"/></div><div class="unit">例</div>
        <div class="comma">，</div>
        <div class="fill-short">出血</div><div class="fill-short-input"><input maxlength="4" value="$!{clinicalApplication.bfzBleedingNumber}" name="bfzBleedingNumber" onkeypress="keyPress()" type="text" style="width:2em" class="center"/></div><div class="unit">例</div>
    </div>
    <div>
        <div class="fill-short">切口感染</div><div class="fill-short-input"><input maxlength="4" value="$!{clinicalApplication.bfzQkgrNumber}" name="bfzQkgrNumber" onkeypress="keyPress()" type="text" style="width:2em" class="center"/></div><div class="unit">例</div>
        <div class="comma">，</div>
        <div class="fill-short">胆瘘</div><div class="fill-short-input"><input maxlength="4" value="$!{clinicalApplication.bfzFistulaNumber}" name="bfzFistulaNumber" onkeypress="keyPress()" type="text" style="width:2em" class="center"/></div><div class="unit">例</div>
     </div>
    <div>
        <div class="fill-short">腹腔感染</div><div class="fill-short-input"><input maxlength="4" value="$!{clinicalApplication.bfzFqgrNumber}" name="bfzFqgrNumber" onkeypress="keyPress()" type="text" style="width:2em" class="center"/></div><div class="unit">例</div>
        <div class="comma">，</div>
        <div class="fill-short">其他</div><div class="fill-short-input"><input maxlength="4" value="$!{clinicalApplication.bfzOtherNumber}" name="bfzOtherNumber" onkeypress="keyPress()" type="text" style="width:2em" class="center"/></div><div class="unit">例</div>
    </div>
</div>
<div class="block" style="text-align: center;"><div class="submit">提交问卷</div>
</body>
<script>

    var webRoot = '${link.contextPath}';
    function myNumberic(e,len) {
        var obj=e.srcElement || e.target;
        var dot=obj.value.indexOf(".");
        len =(typeof(len)=="undefined")?2:len;
        var  key=e.keyCode|| e.which;
            if(key==8 || key==9 || key==46 || (key>=37  && key<=40))//这里为了兼容Firefox的backspace,tab,del,方向键
                 return true;
            if (key<=57 && key>=48) { //数字
                if(dot==-1)//没有小数点
                     return true;
                else if(obj.value.length<=dot+len)//小数位数
                     return true;
            } else if((key==46) && dot==-1){//小数点
                return true;
        }       
        return false;
    }
	
	function keyPress() {    
         var keyCode = event.keyCode;    
         if ((keyCode >= 48 && keyCode <= 57)){    
             event.returnValue = true;    
         } else {    
             event.returnValue = false;    
         }    
    } 
	
	$("input[name=gndgProportion]").blur(function(){
	      var gndgProportion=$(this).val();
		  if(gndgProportion!=""){
		     if(gndgProportion<0||gndgProportion>100){
		         alert("肝内胆管结石数值范围只能为:>0和<=100！");
		     }else if(/^[1-9]\d*(\.\d{1,2})?$/.test(gndgProportion)==false){
		         alert("肝内胆管结石数据不合法！");
		     } 
		 }         
	});
	
	$("input[name=gzlProportion]").blur(function(){
	      var gzlProportion=$(this).val();
		  if(gzlProportion != ""){
		     if(gzlProportion<0||gzlProportion>100){
