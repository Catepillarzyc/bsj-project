html,body {
    background:#e5e9ec;
}
.f-header-wrapper {
    width:100%;
    background:#FFF;
}
.header {
    width: 1000px;
    height: 80px;
    background:#fff;
    margin:0 auto;
    letter-spacing: 0.1em;
}
.header h4 {
    font-size: 24px;
    color:#777;
    line-height: 80px;
    font-weight: normal;
}
.header span {
    float: right;
    margin-left:20px;
    cursor: pointer;
    line-height: none;
    margin-top: 35px;
    margin-right: 10px;
}
.header span:hover{
    color:#01a1e9;
    text-decoration: underline;
}
.header span a:hover{
    color:#01a1e9;
    text-decoration: underline;
}
#f-wrapper {
    width: 100%;
}
.f-container{
    width:1000px;
    margin:40px auto 0;
    background:#FFF;
    border:1px solid #d9dadc;
}
#f-left-items {
    width:150px;
    min-height: 902px;
    float: left;
}

#f-left-items li {
    display: block;
    height: 35px;
    line-height: 35px;
    padding-left: 30px;
}
.first-item {
    padding-top: 20px;
    border-bottom:1px solid #e7e7eb;
}
.one {
    color:#999;
    letter-spacing: 0.1em;
}
.two {
    margin-bottom: 4px;
    cursor: pointer;
}
.selected {
    background-color:#00a0e9 !important;
    color:#fff !important;
}
.f-msgManner {
    background:url("../img/dz.png") no-repeat 10px 8px;
}
.f-countManner {
    background:url('../img/yh.png') no-repeat 10px 8px;
}
.two:hover {
    background-color:#edf7fd;
}
.f-items a:hover {
    text-decoration: none;
}
#f-right-items {
    float: left;
    border-left: 1px solid #e7e7eb;
    background: #FFF;
    min-height: 902px;
}
#f-right-items h3 {
    height: 45px;
    line-height: 45px;
    margin-left: 24px;
    margin-top: 35px;
    font-weight: normal;
}
.list {
    width:800px;
    margin-left: 24px;
    border:1px solid #e7e7eb;
    table-layout: fixed;
    letter-spacing: 0.1em;
}
.list a:hover {
    color:#2aacff;
    text-decoration: none;
}
.list th {
    height:50px;
    line-height: 50px;
    font-weight: normal;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    border-bottom:1px solid #e7e7eb;
    text-align: left;
    text-indent: 10px;
}
.list #f-keyword{
    height: 30px;
    line-height: 30px;
    width:180px;
    font-size:14px;
    outline: none;
    border:1px solid #e7e7eb;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
   
}
.list #f-kind {
   
    width:100px;
    font-size:14px;
    outline: none;
    padding: 3px;
    line-height: 1;
    height: 30px;
    border: 1px solid #e7e7eb;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    margin-left: 10px;
}
.list th img {
    cursor: pointer;
    float:right;
    margin-right:10px;
}
.list td {
    overflow: hidden;
    white-space: nowrap;
    -ms-text-overflow: clip;
    text-overflow: clip;
    height: 40px;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    border-bottom:1px solid #e7e7eb;
}
.list td span{
    color:#2aacff;
    margin-left: 7px;
    cursor: pointer;
}
.list td span:hover {
    text-decoration: underline;
}
.striped {
    background:#f4f5f9;
}
#f-tipbox {
    width: 200px;
    background-color:#fff;
    position: absolute;
    z-index: 99;
    border:1px solid #e5e9ec;
}
#f-tipbox p {   
    line-height: 22px;
    padding:35px 10px 20px 10px;
    text-align:center;
    letter-spacing: 0.1em;
}
#f-tipbox .f-edit , .f-del{
    background:url("../img/qd.png") no-repeat center center;
    display: inline-block;
    cursor: pointer;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
    width:100px;
    height: 50px;
}
#f-tipbox .f-edit:hover {
    background:url('../img/qd2.png') no-repeat center center;
}
#f-tipbox .f-del {
    background:url("../img/qx.png") no-repeat center center;
}
#f-tipbox .f-del:hover {
    background:url('../img/qx2.png') no-repeat center center;
}
#f-newpswBox {
    width: 600px;
    height: 370px;
    background:#fff;
    letter-spacing: 0.1em;
    border: 1px solid #e7e7eb;
    position: absolute;
    z-index: 99;
}
.f-windowBox h4 {
    font-size:16px;
    text-indent: 1em;
    font-weight: normal;
    line-height: 35px;
    height:35px;
    position: relative;
    background: #f2f6f9;
}
#f-newpswBox h4 img {
	position:absolute;
    top: 5px;
    right: 10px;
    cursor: pointer;
}

.f-newpswBox-main {
    padding-top: 35px;
    position: relative;
}
.f-newpswBox-main p {
    margin:0 auto;
    width:500px;
    height: 35px;
    position: relative;
    line-height: 35px;
    margin-bottom: 40px;
}
.f-pswEdit {
	float:left;
    width:320px;
    height: 30px;
    line-height: 30px;
    margin-left: 15px;
    letter-spacing: 0.3em;
}
.psw-text{
	float:left;
    display: inline-block;
    width:90px;
    text-align: right;
    font-size: 15px;
}
.psw-wrong-tip {
    position: absolute;
    left: 155px;
    top: 80px;
    color:#fa3e3e;
}
#psw-wrong-two {
    top:230px;
}
.f-newpswBox-foot img {
    margin-left: 155px;
    cursor: pointer;
}
#f-newpswBox-cancle {
    margin-left:30px;
}
.f-pswEdit-after p{
    margin: 125px auto;
    text-align: center;
    font-size:18px;
    position: relative;
}
.f-pswEdit-after span {
    display: inline-block;
    text-align: center;;
    width: 151px;
    height: 40px;
    line-height: 40px;
    text-indent: 0;
    background:url("../img/kuang1.png");
    position: absolute;
    top: 40px;
    left: 220px;
    cursor: pointer;
}
.f-pswEdit-after span:hover {
    color:#fa3e3e;
    text-decoration: underline;
}
