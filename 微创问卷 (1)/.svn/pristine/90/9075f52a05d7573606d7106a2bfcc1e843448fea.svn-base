@charset "utf-8";
/* CSS Document */
body, div, dl, dt, dd, ul, ol, li,dl, h1, h2, h3, h4, h5, h6,
pre, form, fieldset, input, textarea, p, blockquote, th, td,a,i,span{
	padding: 0;
	margin: 0;
}
body {
	font:14px/2 "Microsoft YaHei";
}

ol, ul {
	list-style: none;
}

h1, h2, h3, h4, h5, h6 {
	font-weight: normal;
	font-size: 100%;
}

.fl {
	float: left;
}

.fr {
	float: right;
}

.clear{
	clear: both;
}
.bl{ 
	display:block;
}
a {
	text-decoration: none;
}
img {
	border: none;
}
table { 
	border-collapse: collapse;
	border-spacing: 0;
}
textarea{
	resize:none;
}
.dn{
	display:none;
}