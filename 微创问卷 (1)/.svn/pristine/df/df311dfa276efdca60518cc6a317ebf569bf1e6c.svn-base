.list-a {
    float: left;
    width: 490px;
    padding-left: 10px;
    outline: hidden;
    margin: 10.5px auto;
}
.list-a li {
    height: 28px;
    overflow: hidden;
    -ms-text-overflow: ellipsis;
    text-overflow: ellipsis;
    line-height: 28px;
    font-size: 13px;
    padding-left: 10px;
}
.list-a li a:hover {
    color:#999;
    text-decoration: none;
}
.list-a a {
    display: inline-block;
    max-width:385px;
    -ms-text-overflow: ellipsis;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
    color:#3c3c3c;
}
.list-a span {
    display: inline-block;
    float: right;
    margin-right: 18px;
    color:#6e6e6e;
    font-size:12px;
}



.mod-m {
