}

.slideContainer {
    width:211px;
    height: 220px;
    overflow: hidden;
    zoom:1;
    position: relative;
    margin:0 auto;
}
.slide-prev {
    position: absolute;
    width: 24px;
    height: 36px;
    left: 0;
    top: 50px;
    background: url(../img/left_arrow.png) no-repeat 0 0;
}
.slide-next {
    position: absolute;
    width: 24px;
    height: 36px;
    right:0;
    top:50px;
    background: url(../img/right_arrow.png) no-repeat 0 0;
}

.slideContainer ul li {
    float: left;
    width: 211px;
    height: 220px;
}
.slideContainer .slideList a {
    display: inline-block;
    margin-top: 10px;
    text-overflow: ellipsis;
    -ms-text-overflow: ellipsis;
    overflow:hidden;
    white-space: nowrap;
    max-width: 211px;
    color:#3C3C3C;
}
.slideContainer ul li img {
    width: 211px;
    height:180px;
    cursor: pointer;
}
.slide-dots {
    text-align: right;
    font-size: 0;
    line-height:0;
    height: 0;
    position: relative;
    bottom: 65px;
    right:10px;

}
.slide-dots span {
    margin:0 3px;
    width: 11px;
    height: 11px;
    display: inline-block;
    cursor: pointer;
    background: #FFF;

}
.slide-dots .dotItemOn {
    background-color: #76b24d;
}

.index_on {
    background:url("../img/index2.png") no-repeat 11px 11px !important;
    background-color:#76b24d !important;
    color:#FFF !important;
}
