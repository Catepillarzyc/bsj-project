@charset "utf-8";
/* CSS Document */
.head{
	height:30px;
	margin-bottom:10px;
	}
.table_main{
	width:95%;
	margin:0 auto;
	}
.bigShow{
	background-color:#e3e3e3;
	position:absolute;
	z-index:99; 
	left:0; top:0; 
	width:100%; height:100%; 
	display:none;
	opacity:0.5;
	filter: alpha(opacity=50);
	-moz-opacity: 0.5;
	}
.window{
	background:#fff;
	position:absolute;
	right:50%;
	top:50%;
	border:1px #CCC solid;
	width:400px;
	margin-right:-200px;
	margin-top:-100px;
	z-index:100;
	}	
.addQuestion{
	margin:10px 50px;;
	float:right;
	background:url(../img/1.png);
	border:none;
	width:63px;
	height:28px;
	display:inline-block;
	text-align:center;
	vertical-align:middle;
	cursor:pointer;
	}
.sure,.cancle{
	margin-left:80px;
	background:url(../img/1.png);
	border:none;
	width:63px;
	height:28px;
	display:inline-block;
	text-align:center;
	vertical-align:middle;
	cursor:pointer;
	}
.addQuestion p,.sure p,.cancle p{
	margin-top:4px;
	}
.click{
	background:url(../img/2.png) !important;
	}
.window_ul{
	margin-left:40px;
	margin-top:10px;
	}
.window_ul li{
	margin-bottom:10px;
	
	}
.sure,.cancle{
	margin-left:80px;
	}
.window_text{
	width:250px;
	}
.window_day{
	width:230px;
	}
.window_textarea{
	width:250px;
	height:50px;
	}
