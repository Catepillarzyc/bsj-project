<script >
	webRoot ="#printUrl";
	$(function(){	
     $(".d_themeLi").each(function(){
			var title = $(this).find("a").text();
			if(title.indexOf("中心概要")>-1){
			  $(this).attr("id","item_zxgy");
			  $(this).find("a").addClass("abled_mod");
			}
			if(title.indexOf("中心成员")>-1){
				$(this).attr("id","item_zxcy");
			}
			if(title.indexOf("单位介绍")>-1){
				$(this).attr("id","item_dwjs");
			}
		})
  })
	
	$(document).delegate(".d_childrenTheme","click",function(){
	    var themeId = $(this).find("input[name=themeId]").val();
		jQuery.ajax({
			url: webRoot + "/weiChuang/getThemeContent.html",
			type: "POST",
			cache: "false",
			async: "false",
			data: {
    			"themeId": themeId
				 },
			dataType: "json",
			success: function(data,text) {
				if(data.list.length>0){
					for(var i=0; i<data.list.length; i++){
						if(data.list[i].title.indexOf("中心概要")>-1){
						
						}
					}
				}
			}
			
		})
	})
	$('.main-part-content').find('.mod-m').eq(0).show();
	$('.zxjj .mod-bd li').click(function(){
		var num = $('.zxjj .mod-bd li').index(this);
		$(".abled_mod").removeClass('abled_mod');
		$(this).find("a").addClass('abled_mod');
		$('.main-part-content').find('.mod-m').hide();
		$('.main-part-content').find('.mod-m').eq(num).show().siblings().hide();
	})
</script>
