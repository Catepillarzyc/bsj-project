body{
    width: 100%;
}

.head{
    width: 100%;
    height: 60px;
    background: #008C9A;
    color: aliceblue;
    text-align: center;
    line-height: 60px;
    font-size: 16px;
    font-weight: 500;
}
.block{
    position: relative;
    float: left;
    width: 20%;
    height: 40px;
}
.arrows{
    position: absolute;
    top:20px;
    right: 5%;
    display: none;
    width: 35px;
}
.nav a{
    display: block;
    width: 100%;
    height: 40px;
    line-height: 40px;
    text-align: center;
    color: #303030;
    font-size: 14px;
}
.finish{
    background: #DCDCDC;
}
.now{
    background:  #D6D3B0;
}
.unfinished{
    background: #F2F2F2;
}
.submit{
    display: block;
    line-height: 30px;
    text-align: center;
    color: #303030;
    font-size: 12px;
    background: #BEDCFC;
    width:85%;
    height: 30px;
    margin: 5px auto;
    -moz-box-shadow:2px 2px 2px #949494;
    -webkit-box-shadow:2px 2px 2px #949494;
    box-shadow:2px 2px 2px #949494;
}