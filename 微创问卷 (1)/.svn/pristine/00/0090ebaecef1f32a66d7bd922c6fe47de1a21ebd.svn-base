body{
    width: 100%;
    background:#ecf0f1;
    overflow-y:hidden;
}

.head{
    width: 100%;
    height: 60px;
    background: #008C9A;
    color: aliceblue;
    text-align: center;
    line-height: 60px;
    font-size: 16px;
    font-weight: 500;
}
iframe {
	display:block;
	width:1000px;
	margin:0 auto;
	background:#fff;
}
.block{
    position: relative;
    float: left;
    width: 25%;
    height: 40px;
    cursor:pointer;
}
.arrows{
    position: absolute;
    top:20px;
    right: 5%;
    display: none;
    width: 35px;
}
.nav {
	margin:15px auto 0;
	width:1000px;
	padding-bottom:5px;
	background:#fff;
}
.nav a{
    display: block;
    width: 100%;
    height: 40px;
    line-height: 40px;
    text-align: center;
    color: #303030;
    font-size: 14px;
    letter-spacing:0.1em;
}
.finish{
    background: #DCDCDC;
}
.now{
    background:  #fff;
    border-bottom:2px solid #008c9a;
    color:#008c9a !important;
}
.unfinished{
    background: #fff;
    border-bottom:1px solid #555;
}
.submit{
    display: block;
    line-height: 30px;
    text-align: center;
    color: #303030;
    font-size: 12px;
    background: #BEDCFC;
    width:85%;
    height: 30px;
    margin: 5px auto;
    -moz-box-shadow:2px 2px 2px #949494;
    -webkit-box-shadow:2px 2px 2px #949494;
    box-shadow:2px 2px 2px #949494;
}